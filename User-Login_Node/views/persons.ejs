
      <!DOCTYPE html>
      <html>
      <head>
          <meta name="viewport" content="width=device-width, initial-scale=1">
      
      </head>
      <body>
          <% if (query.length > 0) { %>
        <div class="row mt-5">
        <div class="col-md-6 m-auto">
            <div class="card card-body text-center">
                <h1><i class="fab fa-node-js fa-3x"></i></h1>
                <h5 class="card-title"> <%= query[0].userName %> </h5>
                <p class="card-text"> Email : <%= query[0].emailId %> </p>
                <p class="card-text"> Phone : <%= query[0].phoneNo %> </p>
            </div>
        </div>
      </div>
      <% } %>
      <% if(query.length == 0) { %>
        <div class="row mt-5">
                <div class="col-md-6 m-auto">
                    <div class="card card-body text-center">
                        <h1><i class="fab fa-node-js fa-3x"></i></h1>
                        <h5 class="card-title"> Email Not Found :( </h5>
                    </div>
                </div>
              </div>
        <% } %>
    
      
      
      </body>
      </html>
